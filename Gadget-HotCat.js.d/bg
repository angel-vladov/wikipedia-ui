//<source lang="javascript">
// Localization of HotCat
if (window.HotCat) {
	$.extend(HotCat.messages, {
		cat_removed:      'Премахване на ['+'[Категория:$1]]',
		template_removed: 'Премахване на {'+'{['+'[Категория:$1]]}}',
		cat_added:        'Добавяне на ['+'[Категория:$1]]',
		cat_keychange:    'Нов ключ за ['+'[Категория:$1]]: „$2“',
		cat_notFound:     'Категорията „$1“ не беше открита',
		cat_exists:       'Категорията „$1“ вече е налична и не беше добавена отново.',
		cat_resolved:     '(оправено пренасочване към ['+'[Категория:$1]])',
		uncat_removed:    'премахване на {'+'{uncategorized}}',
		using:            ', ползвайки [[MediaWiki:Gadget-HotCat.js|HotCat]]',
		multi_change:     '$1 категории',
		commit:           'Запис',
		ok:               'Готово'; // OK
		cancel:           'Отказ',
		multi_error:      'Текстът на страницата не можа да бъде зареден и промените ви не могат да бъдат съхранени.'
	});

	$.extend(HotCat.tooltips, {
		change:  'Промяна',
		remove:  'Премахване',
		add:     'Добавяне на категория',
		restore: 'Отмяна на промените',
		undo:    'Отмяна на промените',
		down:    'Отваряне и показване на подкатегориите',
		up:      'Отваряне и показване на родителските подкатегории'
	});

	$.extend(HotCat.engine_names, {
		searchindex: 'Търсене', // Search index
		pagelist   : 'Списък на страници',
		combined   : 'Комбинирано търсене',
		subcat     : 'Подкатегории',
		parentcat  : 'Родителски категории'
	});

	$.extend(HotCat, {
		categories   : 'Категории',
		multi_tooltip: 'Промяна на няколко категории'
	});
}
