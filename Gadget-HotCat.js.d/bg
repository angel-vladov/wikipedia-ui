// Localizations of a few HotCat user interface texts.
if (typeof (HotCat) != 'undefined') {
  HotCat.messages.cat_removed   = 'Премахване на [[Категория:$1]]';
  HotCat.messages.template_removed  = 'Премахване на {{[[Категория:$1]]}}';
  HotCat.messages.cat_added     = 'Добавяне на [[Категория:$1]]';
