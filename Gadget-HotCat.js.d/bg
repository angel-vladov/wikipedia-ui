  HotCat.messages.cat_notFound  = 'Категорията „$1“ не беше открита';
  HotCat.messages.cat_exists    = 'Категорията „$1“ вече е налична и не беше добавена отново.';
  HotCat.messages.cat_resolved  = "(оправено пренасочване към \[\[:Категория:$1|$1\]\])";
  HotCat.messages.uncat_removed = "премахване на {" + "{uncategorized}}";
  HotCat.messagesusing          = "ползвайки [[MediaWiki:Gadget-HotCat.js|HotCat.js]]";
  HotCat.messages.multi_change  = '$1 категории';
  HotCat.messages.commit        = "Запис";
  HotCat.messages.ok            = "Готово"; // OK
  HotCat.messages.cancel        = "Отказ";
  HotCat.messages.multi_error   = "Текстът на страницата не можа да бъде зареден и промените ви не могат да бъдат съхранени.";

  HotCat.category_regexp = '[Cc][Aa][Tt][Ee][Gg][Oo][Rr][Yy]|[Кк]атегория';
  HotCat.category_canonical = "Категория";
  HotCat.categories  = "Категории";
  HotCat.template_regexp = '[Tt][Ee][Mm][Pp][Ll][Aa][Tt][Ee]|[Шш]аблон';

  HotCat.engine_names.searchindex = "Търсене"; // Search index
  HotCat.engine_names.pagelist    = 'Списък на страници';
  HotCat.engine_names.combined    = 'Комбинирано търсене';
  HotCat.engine_names.subcat      = 'Подкатегории';
  HotCat.engine_names.parentcat   = 'Родителски категории';

  HotCat.tooltips.change = "Промяна";
  HotCat.tooltips.remove = "Премахване";
  HotCat.tooltips.add = "Добавяне на категория";
  HotCat.tooltips.restore = "Отмяна на промените";
  HotCat.tooltips.undo = "Отмяна на промените";
  HotCat.tooltips.down = "Отваряне и показване на подкатегориите";
  HotCat.tooltips.up = "Отваряне и показване на родителските подкатегории";

  HotCat.multi_tooltip = "Промяна на няколко категории";
